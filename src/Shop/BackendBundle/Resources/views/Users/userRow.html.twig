<tr data-user="{{user.id}}">
    <td>{{user.firstName}}</td>
    <td>{{user.lastName}}</td>
    <td>{{user.email}}</td>
    <td>
        <a href="{{path('shop_backend_users_edit', {'id': user.id})}}" class="edit">
            {% trans %}edit{% endtrans %}
        </a>
    </td>
    <td>
        <a href="{{path("shop_backend_users_delete", {'id': user.id})}}" class="delete" data-confirm="{% trans with {'%name%': user.name} %}user.delete.confirm{% endtrans %}">
            {% trans %}delete{% endtrans %}
        </a>
    </td>
</tr>